{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": 0,
    "no-console": "off",
    "object-curly-spacing": ["error", "always"],
    "quotes": ["error", "double"]
  }
}

